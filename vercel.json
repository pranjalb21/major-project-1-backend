{
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@vercel/node" }],
    "rewrites": [
        { "source": "/products/(.*)", "destination": "index.js" },
        { "source": "/wishlist/(.*)", "destination": "index.js" },
        { "source": "/cart/(.*)", "destination": "index.js" },
        { "source": "/address/(.*)", "destination": "index.js" },
        { "source": "/order/(.*)", "destination": "index.js" }
    ]
}
